{
	"compilerOptions": {
	  "target": "ES2020",
	  "module": "CommonJS",          // Change from ESNext to CommonJS
	  "moduleResolution": "Node",
	  "outDir": "./out",
	  "strict": true,
	  "esModuleInterop": true,
	  "skipLibCheck": true,
	  "forceConsistentCasingInFileNames": true,
	  "resolveJsonModule": true,
	  "noImplicitAny": false
	},
	"include": [
	  "src/**/*.ts"
	],
	"exclude": [
	  "node_modules"
	]
  }
  